package com.st.smartrash.common;

public class CategoryTranslation {
	public String categoryTranslation(String category_name) {
		switch(category_name) {
		case "can": category_name = "캔";
		case "fluorescent_light": category_name = "형광등";
		case "glass": category_name = "유리";
		case "paper": category_name = "종이";
		case "plastic": category_name = "플라스틱";
		case "polystyrene": category_name = "스티로폼";
		case "vinyl": category_name = "비닐";
		}
		return category_name;
	}
}
